# VectorOS Backend Environment Variables

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/vectoros

# JWT
JWT_SECRET=your-super-secret-jwt-key-min-32-characters-long

# AI Core
AI_CORE_URL=http://localhost:8000

# Server
PORT=3001
NODE_ENV=development

# Redis (optional)
REDIS_URL=redis://localhost:6379

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info

# CORS
CORS_ORIGINS=http://localhost:3000

# Features
ENABLE_METRICS=true
ENABLE_CACHING=true
ENABLE_RATE_LIMITING=true
